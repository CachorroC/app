    "eslint": "8.52.0",
    "eslint-config-next": "14.0.1",
    "material-symbols": "^0.13.2",
    "mongodb": "^6.3.0",
    "next": "14.1.2-canary.2",
    "next-auth": "beta",
    "prettier": "^3.2.5",
    "prisma": "5.10.2",

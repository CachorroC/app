{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "styles:tcm": "tcm src",
    "sass": "sass --no-source-map src:src && tcm src",
    "lint": "next lint --fix && next lint",
    "prebuild": "pnpm sass && pnpm lint",
    "devbuild": "pnpm prebuild && next build",
    "build": "next build",
    "dev": "URL=beta.rsasesorjuridico.com NEXTAUTH_URL=https://beta.rsasesorjuridico.com next dev",
    "start": "next start",
    "db:migrate": "prisma migrate dev --name init --create-only && prisma generate",
    "db:push": "prisma db push",
    "json-to-prisma": "json-to-prisma-schema-convertor convert --inputPath='./prisma/schema.json' --outputPath='./prisma/new/schema.prisma'",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@prisma/client": "^5.2.0",
    "@types/bcrypt": "^5.0.0",
    "@types/node": "20.5.9",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "bcrypt": "^5.1.1",
    "dayjs": "^1.11.9",
    "eslint": "8.48.0",
    "eslint-config-next": "13.4.19",
    "material-symbols": "^0.11.0",
    "mongodb": "^6.0.0",
    "next": "13.4.19",
    "next-auth": "^4.23.1",
    "prisma": "^5.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.46.1",
    "sass": "^1.66.1",
    "server-only": "^0.0.1",
    "typed-css-modules": "^0.7.2",
    "typescript": "5.2.2",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.4.1",
    "@storybook/addon-interactions": "^7.4.1",
    "@storybook/addon-links": "^7.4.1",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.4.1",
    "@storybook/nextjs": "^7.4.1",
    "@storybook/react": "^7.4.1",
    "@storybook/testing-library": "^0.2.0",
    "eslint-plugin-storybook": "^0.6.13",
    "storybook": "^7.4.1"
  }
}

{
  "name": "app",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "styles:tcm": "tcm src",
    "sass": "sass src:src && tcm src",
    "lint": "next lint --fix && next lint",
    "prebuild": "pnpm run sass && pnpm run lint",
    "devbuild": "pnpm run prebuild && next build",
    "build": "next build",
    "dev": "URL=beta.rsasesorjuridico.com NEXTAUTH_URL=https://beta.rsasesorjuridico.com PREFIX=beta next dev",
    "start": "next start",
    "db:migrate": "prisma migrate dev --name init --create-only && prisma generate",
    "db:push": "prisma db push",
    "json-to-prisma": "json-to-prisma-schema-convertor convert --inputPath='./prisma/schema.json' --outputPath='./prisma/new/schema.prisma'"
  },
  "dependencies": {
    "@prisma/client": "5.5.2",
    "@types/node": "20.8.10",
    "@types/react": "^18.2.34",
    "@types/react-dom": "^18.2.14",
    "eslint": "8.52.0",
    "eslint-config-next": "14.0.1",
    "material-symbols": "^0.13.2",
    "mongodb": "^6.2.0",
    "next": "14.0.1",
    "prisma": "5.5.2",
    "react": "canary",
    "react-currency-input-field": "^3.6.12",
    "react-dom": "canary",
    "react-hook-form": "^7.47.0",
    "sass": "^1.69.5",
    "server-only": "^0.0.1",
    "typed-css-modules": "^0.8.0",
    "typescript": "next",
    "zod": "^3.22.4"
  }
}

{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "styles:tcm": "tcm src",
    "sass": "sass src:src && tcm src",
    "lint": "next lint --fix && next lint",
    "prebuild": "pnpm run sass && pnpm run lint",
    "devbuild": "pnpm run prebuild && next build",
    "build": "next build",
    "dev": "URL=beta.rsasesorjuridico.com NEXTAUTH_URL=https://beta.rsasesorjuridico.com PREFIX=beta next dev",
    "start": "next start",
    "db:migrate": "prisma migrate dev --name init --create-only && prisma generate",
    "db:push": "prisma db push",
    "json-to-prisma": "json-to-prisma-schema-convertor convert --inputPath='./prisma/schema.json' --outputPath='./prisma/new/schema.prisma'"
  },
  "dependencies": {
    "@material/web": "^1.0.1",
    "@prisma/client": "^5.5.2",
    "@types/bcrypt": "^5.0.1",
    "@types/node": "20.8.6",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "bcrypt": "^5.1.1",
    "eslint": "8.52.0",
    "eslint-config-next": "13.5.5",
    "material-symbols": "^0.13.1",
    "mongodb": "^6.2.0",
    "next": "latest",
    "prisma": "^5.5.2",
    "react": "latest",
    "react-dom": "latest",
    "react-hook-form": "^7.47.0",
    "sass": "^1.69.5",
    "server-only": "^0.0.1",
    "typed-css-modules": "^0.8.0",
    "typed-scss-modules": "^7.1.4",
    "typescript": "next",
    "zod": "^3.22.4"
  },
  "pnpm": {
    "overrides": {
      "postcss@<7.0.36": ">=7.0.36",
      "postcss@<8.4.31": ">=8.4.31"
    }
  }
}

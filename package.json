{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "styles:tcm": "tcm src",
    "sass": "sass --no-source-map src:src && tcm src",
    "lint": "next lint --fix --strict && next lint --strict",
    "devbuild": "sass --no-source-map src:src && tcm src && eslint --fix . && next build",
    "build": "sass --no-source-map src:src && tcm src && next build",
    "dev": "sass --no-source-map src:src && tcm src && URL=beta.rsasesorjuridico.com NEXTAUTH_URL=https://beta.rsasesorjuridico.com next dev ",
    "start": "next start",
    "db:migrate": "prisma migrate dev --name init --create-only && prisma generate",
    "db:push": "prisma db push",
    "json-to-prisma": "json-to-prisma-schema-convertor convert --inputPath='./prisma/schema.json' --outputPath='./prisma/new/schema.prisma'"
  },
  "dependencies": {
    "@base-ui-components/react": "1.0.0-alpha.7",
    "@prisma/client": "^6.16.2",
    "@stylistic/eslint-plugin": "^2.13.0",
    "@types/node": "20.6.3",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@types/web-push": "^3.6.4",
    "@yudiel/react-qr-scanner": "latest",
    "bcrypt": "^5.1.1",
    "material-symbols": "^0.13.2",
    "mongodb": "^6.20.0",
    "next": "15.6.0-canary.47",
    "prisma": "^5.22.0",
    "react": "19.1.1",
    "react-currency-input-field": "^3.10.0",
    "react-dom": "19.1.1",
    "react-error-boundary": "^4.1.2",
    "react-hook-form": "^7.62.0",
    "sass": "^1.92.1",
    "server-only": "^0.0.1",
    "typed-css-modules": "^0.9.1",
    "typed-scss-modules": "^8.1.1",
    "typescript": "5.2.2",
    "web-push": "^3.6.7",
    "xlsx": "^0.18.5",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "eslint": "^9.35.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.4.0",
    "jiti": "^2.5.1",
    "typescript-eslint": "^8.44.0"
  }
}

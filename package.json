{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "styles:tcm": "tcm src",
    "sass": "sass --no-source-map src:src && tcm src",
    "lint": "next lint --fix --strict && next lint --strict",
    "devbuild": "sass --no-source-map src:src && tcm src && eslint --fix . && next build",
    "build": "sass --no-source-map src:src && tcm src && next build",
    "dev": "sass --no-source-map src:src && tcm src && URL=beta.rsasesorjuridico.com NEXTAUTH_URL=https://beta.rsasesorjuridico.com next dev ",
    "start": "next start",
    "db:migrate": "prisma migrate dev --name init --create-only && prisma generate",
    "db:push": "prisma db push",
    "json-to-prisma": "json-to-prisma-schema-convertor convert --inputPath='./prisma/schema.json' --outputPath='./prisma/new/schema.prisma'",
    "db:test": "tsx scripts/test-database.ts",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@base-ui-components/react": "1.0.0-rc.0",
    "@eslint/js": "^9.39.2",
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "7.3.0",
    "@stylistic/eslint-plugin": "^5.7.1",
    "@types/node": "25.2.0",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@yudiel/react-qr-scanner": "latest",
    "bcrypt": "^6.0.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^17.3.0",
    "jiti": "^2.6.1",
    "material-symbols": "^0.40.2",
    "mongodb": "^7.1.0",
    "next": "^16.1.6",
    "pg": "^8.18.0",
    "prisma": "^7.3.0",
    "react": "19.2.4",
    "react-currency-input-field": "^4.0.3",
    "react-dom": "19.2.4",
    "react-error-boundary": "^6.1.0",
    "react-hook-form": "^7.71.1",
    "sass": "^1.97.3",
    "server-only": "^0.0.1",
    "tsx": "^4.21.0",
    "typed-css-modules": "^0.9.1",
    "typed-scss-modules": "^8.1.1",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.54.0",
    "web-push": "^3.6.7",
    "xlsx": "^0.18.5",
    "zod": "^4.3.6"
  },

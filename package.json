{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "styles:tcm": "tcm src",
    "sass": "sass --no-source-map src:src && tcm src",
    "lint": "next lint --fix && next lint",
    "prebuild": "bun sass && bun lint",
    "devbuild": "bun prebuild && next build",
    "build": "next build",
    "dev": "URL=beta.rsasesorjuridico.com NEXTAUTH_URL=https://beta.rsasesorjuridico.com PREFIX=beta next dev",
    "start": "next start",
    "db:migrate": "prisma migrate dev --name init --create-only && prisma generate",
    "db:push": "prisma db push",
    "json-to-prisma": "json-to-prisma-schema-convertor convert --inputPath='./prisma/schema.json' --outputPath='./prisma/new/schema.prisma'"
  },
  "dependencies": {
    "@prisma/client": "^5.3.1",
    "@types/bcrypt": "^5.0.0",
    "@types/node": "20.6.3",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "bcrypt": "^5.1.1",
    "eslint": "8.49.0",
    "eslint-config-next": "13.5.2",
    "material-symbols": "^0.13.0",
    "mongodb": "^6.1.0",
    "next": "latest",
    "prisma": "^5.3.1",
    "react": "latest",
    "react-dom": "latest",
    "react-hook-form": "^7.46.1",
    "sass": "^1.67.0",
    "server-only": "^0.0.1",
    "typed-css-modules": "^0.7.2",
    "typed-scss-modules": "^7.1.4",
    "typescript": "next",
    "zod": "^3.22.2"
  }
}

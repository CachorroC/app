{
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "incremental": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "module": "esnext",
    "moduleResolution": "bundler",
    "noEmit": true,
    "noImplicitAny": true,
    "paths": {
      "#@/*": [
        "./src/*"
      ],
      "auth": [
        "./src/auth"
      ],
      "components/*": [
        "./src/components/*"
      ],
      "project/*": [
        "./src/lib/project/*"
      ],
      "types/*": [
        "./src/lib/types/*"
      ]
    },
    "plugins": [
      {
        "name": "next"
      }
    ],
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "ESNext"
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "public/service-worker.js",
    "src/app/Contabilidad/idkts",
    ".vscode/idk.ts",
    "/home/cachorro_cami/Projects/com/rsasesorjuridico/app/.next/types/**/*.ts"
  ]
}

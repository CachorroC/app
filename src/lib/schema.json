        },
        "category": {
          "$ref": "#/definitions/Category"
        },
        "categoryTag": {
          "type": "integer"
        },
        "deudor": {
          "$ref": "#/definitions/Deudor"
        },
        "numero": {
          "type": "integer"
        },
        "llaveProceso": {
          "type": "string"
        },
        "tipoProceso": {
          "$ref": "#/definitions/TipoProceso"
        },
        "idProceso": {
          "type": "integer"
        },
        "fecha": {
          "type": "string",
          "format": "date-time"
        },
        "ultimaActuacion": {
          "$ref": "#/definitions/UltimaActuacion"
        }

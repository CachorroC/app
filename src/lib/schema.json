        "cedula",
        "direccion",
        "email",
        "primerApellido",
        "primerNombre",
        "segundoApellido",
        "segundoNombre",
        "tel"
      ],
      "title": "Deudor"
    },
    "Tel": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "fijo": {
